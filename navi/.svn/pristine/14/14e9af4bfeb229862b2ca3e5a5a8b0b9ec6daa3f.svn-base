<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.navi.alarm.dao.AlramTypeDAO">
	<select id="selectAlramType" resultType="AlarmTypeVO">
		SELECT
		    altype_id
		  , altype_name
		  , altype_cn
		FROM
		    alarm_type
		where altype_id= #{altypeId}
	</select>
</mapper>