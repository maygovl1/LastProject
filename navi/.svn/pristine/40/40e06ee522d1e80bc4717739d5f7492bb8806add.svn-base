@charset "UTF-8";
@import url("../../../font/GmarketSans/GmarketSans.css");
@import url("../../../font/NotoSansKR/NotoSansKR.css"); 
@import url("../../../font/CaviarDreams/CaviarDreams.css");
@import url("../../common/common_icon.css");
@import url("../../common/common_table.css");
@import url("../../common/common_chat.css");
@import url("../../common/calendar.css");
 /* common */
* {
    font-family: "NotoSansKR", sans-serif;
    line-height: 1.3;
    box-sizing: border-box !important;
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}

html, body {
    width: 100%;
   /*  height: 100%; */
    /* overflow: hidden; */
    font-size: 15px;
}

body{overflow-x: hidden;}
a, button, span, em, strong {
    display: inline-block;
}

a {
    display: inline-block;
    text-decoration: none;
    color: #333;
    transition: 0.3s;
}

a:hover, a:focus ,button:hover, button:focus{  border:transparent; outline:transparent}

a:hover, a:focus {
    color: #0d6efd;
    font-weight: bold;
    text-decoration: none;
  
}

li {
    list-style: none;
}

.skip, .sr-only {
    display: block;
    font-size: 0;
    clip: rect(0, 0, 0, 0);
    margin: -1px;
    padding: 0;
    position: absolute;
}

em, i {
    font-style: normal;
}

.tit {
    color: #fff;
    font-family: "CaviarDreams";
    word-break: break-all;
}

.tit.yel {
    color: #ffe957;
}

.tit.blu {
    color: #200a97;
}

.s-tit {
    font-size: 2rem;
}

.m-tit {
    font-size: 7rem;
}

.img {
    position: relative;
}

.img .img-in {
    position: relative;
    width: 100%;
    height: 100%;
}

.img .img-in .imgcon {
    background-position: center center;
    background-repeat: no-repeat;
    background-size: contain;
    display: inline-block;
    position: absolute;
    width: 100%;
    height: 100%;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.wrap {
    position: relative;
    width: 100%;
    /* height: 100%; */
    display: block;
    overflow: hidden;
}




.object {
    position: absolute;
    display: inline-block;
    background-position: center center;
    background-repeat: no-repeat;
    background-size: auto;
}

.category {
    background-color: #fff;
    font-size: 20px;
    font-weight: 500;
    line-height: 1;
    border-radius: 5px;
}

.category.cate1 {
    background-color: #8cc84b;
}

.category.cate2 {
    background-color: #200a97;
}

.category.cate3 {
    background-color: #b6aef4;
}

.category.cate4 {
    background-color: #ffe957;
}
