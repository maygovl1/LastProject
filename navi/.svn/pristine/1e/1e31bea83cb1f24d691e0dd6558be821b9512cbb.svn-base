/* quick */


.quick-box {
    position: fixed;
    transition: all .3s;
    bottom: 0px;
    left:50%;
    transform:translateX(-50%);
    width: 100%;
    height: 85px;
    max-width: 800px;
    z-index: 999;
    opacity:0; 
    visibility:hidden;
}
.quick-box.show{
	 opacity:1; 
    visibility:visible;
}

.quick-box .icon {
    margin: 0 auto;
    background-size: auto 100%;
}

.quick-box .btn {
    border: transparent;
    /* border-radius: 6px; */
    transition: all .3s;
    background-color: #333840;
    height: 100%;
    z-index: 3;
}

.quick-box.active {width:calc(100% - 100px);height: 130px; bottom: 0;  
}
.quick-box * {
    color: #fff;
    background-color: #333840
}

.quick-box .ul {
    padding: 0;
    border-radius: 6px;
    overflow: hidden;
    height:100%;
    width: 100%;
    text-align: center;
    white-space: nowrap;
}


.quick-box .ul li{display: inline-block;height: 100%;float:left;width: calc(100% / 8);}
.quick-box .ul .btn{height:100%;display: inline-flex;flex-direction: column;align-items: center;padding: 5px 10px;width: 100%;border-radius: 0;}
.quick-box .txt{line-height: 1.1;}
.quick-box li:not(:last-child) .txt{margin-top: 6px;}

.quick-box .btn * {
    background-color: transparent;
    /* line-height: 1.1; */
}

.quick-box .btn:hover,.quick-box .btn:focus {
    background-color: #5347DB
}

.quick-box .icon {
    width: 50px;
    height: 25px;
    display: block;
    margin: 8px 0 4px;
    min-height: 7px;
}


.chat-box {
    /* bottom: 0; */
    right: 10px;
    z-index:998;
    position: fixed;
    right: 25px;
    top: 50%;
    transform: translateY(-50%);
}


.chat-box .btn {
    border: transparent;
    /* border-radius: 6px; */
    transition: all .3s;
    background-color: #333840;
    z-index: 3;
}


.chat-box .btn.active,.chat-box .btn:hover,.chat-box .btn:focus {
    background-color: #5347DB;
    color: #fff
}

.chat-box .chat-conbox {
    opacity: 0;
    visibility: hidden;
    transition: all .3s;
    /* right: 20px; */
    width: 500px;
    /* width: 100%; */
    /* padding-right: 10px; */
}
.chat-box.active{width:500px}
.chat-box.active .chat-conbox {
    opacity: 1;
    visibility: visible;
  
}

.chat-box .icon {
    background-size: cover
}

.chat-inner {
    position: relative
}

.chat-tit {
    height: 76px;
    width: 76px;
    background-image: url("data:image/svg+xml,%3Csvg width='44' height='44' viewBox='0 0 44 44' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='M32.6628 24.5509C33.5831 24.5509 34.3294 23.8045 34.3294 22.8842C34.3294 21.9639 33.5831 21.2175 32.6628 21.2175C31.7424 21.2175 30.9961 21.9639 30.9961 22.8842C30.9961 23.8045 31.7424 24.5509 32.6628 24.5509Z' stroke='white' stroke-width='2' stroke-linecap='round' stroke-linejoin='round'/%3E%3Cpath d='M22.6628 24.5509C23.5831 24.5509 24.3294 23.8045 24.3294 22.8842C24.3294 21.9639 23.5831 21.2175 22.6628 21.2175C21.7424 21.2175 20.9961 21.9639 20.9961 22.8842C20.9961 23.8045 21.7424 24.5509 22.6628 24.5509Z' stroke='white' stroke-width='2' stroke-linecap='round' stroke-linejoin='round'/%3E%3Cpath d='M12.6628 24.5509C13.5832 24.5509 14.3294 23.8045 14.3294 22.8842C14.3294 21.9639 13.5832 21.2175 12.6628 21.2175C11.7423 21.2175 10.9961 21.9639 10.9961 22.8842C10.9961 23.8045 11.7423 24.5509 12.6628 24.5509Z' stroke='white' stroke-width='2' stroke-linecap='round' stroke-linejoin='round'/%3E%3Cpath d='M22 42.2175C33.0456 42.2175 42 33.2631 42 22.2175C42 11.1718 33.0456 2.21747 22 2.21747C10.9543 2.21747 2 11.1718 2 22.2175C2 25.8603 2.97394 29.2757 4.67564 32.2175L3 41.2175L12 39.5419C14.9417 41.2435 18.3572 42.2175 22 42.2175Z' stroke='white' stroke-width='3' stroke-linecap='round' stroke-linejoin='round'/%3E%3C/svg%3E%0A");
    background-repeat: no-repeat;
    background-position: center center;
   
}

.chat-conbox {
    position: fixed;
    top: 50%;
    left:50%;
    transform:translate(-50%,-50%);
    background-color: #fff;
    width: 500px;
    /* height: 500px; */
    border-radius: 30px;
}

.chat-tit.active {
    background-color: #5347DB
}

.chat-tit.active + .chat-conbox {
    opacity: 1;
    visibility: visible;
    width: 300px;
    height: auto
}


