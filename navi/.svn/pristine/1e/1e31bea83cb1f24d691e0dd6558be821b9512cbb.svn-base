    height: 100%;
    margin: 0
}

.img-card .category {
    padding: 14px 10px 10px;
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    z-index: 6;
    color:#333;
    font-size:20px
}

.img-card .object.star {
