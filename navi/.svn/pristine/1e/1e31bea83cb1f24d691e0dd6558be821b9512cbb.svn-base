    /* overflow: hidden; */

}

.section .box {
    height: 555px;
    text-align: left
}

.section .img {
    width: 44%;
    /* margin-top: -100px; */
}

.section .box.text {
    width: 46%
}

.section .box.text .tit {
    text-shadow: 0 0 0px rgba(0,0,0,0.9);
    font-weight: 700;
    line-height: 1;
    display: block;
    color:#fff;
    font-size: 90px;
    margin-bottom: 10px;
}
.section .box.text .tit.yel{
