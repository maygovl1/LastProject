    /* color: #FFE957; */
}

.section .object {
    position: absolute;
    width: 464px;
    height: 488px;
    animation: rotate_obj 15s linear infinite; /* 애니메이션 속성 수정 */
    transform-origin: 50% 50%;
}

@keyframes rotate_obj {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(360deg);
    }
}


