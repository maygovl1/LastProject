.sc3 .inner {
    flex-direction: column;
    /* margin-top: -200px; */
    /* position: relative; */
    /* z-index: 10; */
}

.sc3 {
    background: rgba(32,10,151,0.2)
}

.img-card-list * {
    font-family: "Gmarketsans";
    justify-content: center;
    text-align: center
}

.img-card .item {
    text-align: center;
    padding: 30px
}

.img-card {
    position: relative;
    height: 460px;
    max-width: 450px;
    width: 100%;
    margin: 0 auto 25px;
    padding: 0 25px 10px;
}

.img-card .img-in {
    display: block;
    width: 100%;
    height: 100%;
    position: relative;
    overflow: hidden
}

.img-card .img {
    display: block;
    width: auto;
    height: 100%;
    margin: 0;
    border-radius: 50% !important;
}

.img-card .category {
    padding: 14px 10px 10px;
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    z-index: 6;
    color:#333;
    /* border: 3px solid #000; */
    font-size:20px;
    box-shadow: 1px 3px 7px rgba(0,0,0,0.5);
}



@keyframes animate {
  0%
  {
    transform: translateY(0px);
  }
  50%
  {
    transform: translateY(-20px);
  }
  100%
  {
    transform: translateY(0px);
  }
}



/* star */
.object.star {
    width: 86px;
    height: 86px;
    left: 50%;
    top: 0;
    transform: translate(-179%,0);
    animation: none;
    z-index: 3;
   
}
.object.star .con{position:relative; display:block; width:100%;height:100%;}


.object.star .bigStar,
.object.star .smStar{
	background-position:center center; background-repeat:no-repeat; background-size:100%;position:absolute;
	animation: animate .8s infinite;	
	
}


.object.star .bigStar{
	background-image: url("data:image/svg+xml,%3Csvg width='85' height='82' viewBox='0 0 85 82' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='M13.3826 12.2447C12.2302 5.52769 19.0975 0.281706 25.275 3.16005L38.6804 9.40622C41.1295 10.5473 43.9766 10.448 46.3402 9.13898L59.1814 2.02709C65.3376 -1.38241 72.7191 3.79004 71.6159 10.7403L69.4476 24.4009C69.0072 27.1758 69.9754 29.9881 72.0306 31.9038L82.195 41.3781C87.2436 46.084 84.8234 54.5301 78.0487 55.8481L63.0559 58.7647C60.4663 59.2685 58.2608 60.9533 57.0938 63.3192L50.3368 77.0171C47.2835 83.2068 38.4982 83.3205 35.2858 77.212L28.8181 64.9137C27.5104 62.427 25.0517 60.7534 22.2587 60.4485L8.50869 58.9479C1.51295 58.1843 -1.53613 49.7023 3.37238 44.6595L13.611 34.1407C15.4956 32.2046 16.3402 29.4838 15.8833 26.8209L13.3826 12.2447Z' fill='%23FFCA1C'/%3E%3C/svg%3E%0A");
	width: 85px;
	height: 82px;
	left: 0;
	top: 0;
	display:block;
	animation-delay: .2s;
}



.object.star .smStar{
	display:block;
	background-image: url("data:image/svg+xml,%3Csvg width='44' height='44' viewBox='0 0 44 44' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='M13.7378 5.22351C14.2473 -0.207621 21.409 -1.81383 24.1891 2.87952L26.3033 6.44857C27.5466 8.5475 30.0001 9.60713 32.3805 9.07327L36.4282 8.16548C41.7509 6.97171 45.4916 13.2866 41.8871 17.381L39.146 20.4946C37.534 22.3256 37.2844 24.9865 38.5278 27.0854L40.6419 30.6544C43.4221 35.3478 38.5722 40.8568 33.5643 38.6939L29.7561 37.0492C27.5165 36.0819 24.9088 36.6668 23.2968 38.4978L20.5558 41.6114C16.9512 45.7058 10.2132 42.7957 10.7227 37.3646L11.1101 33.2345C11.3379 30.8056 9.9759 28.5063 7.73631 27.539L3.92808 25.8942C-1.07979 23.7314 -0.394289 16.4238 4.92847 15.2301L8.97615 14.3223C11.3566 13.7884 13.1225 11.7825 13.3503 9.35361L13.7378 5.22351Z' fill='%23FFF500' fill-opacity='0.85'/%3E%3C/svg%3E%0A");
	width:44px;
	height: 44px;
	z-index:-1

}

.object.star .smStar.star1{
    left: -4px;
    bottom: -11px;
    z-index: -1;
     animation-delay: .1s;
}


.object.star .smStar.star2{
    top: 5px;
    right: -23px;
    z-index: -1;
    animation-delay: .3s;
}


