package kr.or.navi.admin.form.controller;

import org.springframework.stereotype.Controller;
import org.springframework.ui.Model;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
@RequestMapping("/admin")
public class AdminFormController {

	@GetMapping("/form")
	public String formPage(Model model) {
		return "admin/formboard/adminFormList";
	}

	@GetMapping("/form/detail")
	public String formDitailPage(Model model) {
		return "admin/formboard/adminFormDetail";
	}

	@GetMapping("/form/add")
	public String formAddPage(Model model) {
		return "admin/formboard/adminFormAdd";
	}

	@GetMapping("/form/mod")
	public String formModPage(Model model) {
		return "admin/formboard/adminFormMod";
	}
}
