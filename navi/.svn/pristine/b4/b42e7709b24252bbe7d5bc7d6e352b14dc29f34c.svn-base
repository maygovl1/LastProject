.footer {
  background-color: #200a97;
  padding: 40px 70px;
}
.footer * {
  color: #fff;
  font-family: "NotoSansKR";
  line-height: 1.3;
  word-break: keep-all;
}
.footer-inner {
  max-width: calc(1920px - 60px);
  width: 100%;
  margin: 0 auto;
  justify-content: space-between;
}
.footer_logo {
  font-size: 40px;
  font-weight: bold;
  position: relative;
  padding-top: 15px;
  font-family: "Gmarketsans";
  margin-bottom: 10px;
}
.footer_logo i {
  background: url("../../../images/layout/navi.png") no-repeat center center;
  width: 39px;
  height: 39px;
  display: inline-block;
  position: absolute;
  top: -9px;
  right: -29px;
  transform: rotate(-4deg);
  background-size: 100%;
}
.footer_ul {
  padding-left: 0;
  position: relative;
  margin: 0;
}

.footer_ul .li {
  display: inline-block;
  float: left;
  position:relative;
}
.footer_ul li a {
  font-size: 16px;
  position: relative;
}

.footer_ul li:not(:last-child) a {
  padding-right: 20px;
}
.footer_ul li:not(:last-child) a:after {
  content: "";
  display: inline-block;
  position: absolute;
  top: 5px;
  right: 10px;
  width: 1px;
  height: 12px;
  background-color: #fff;
}

.footer_ul li a:hover,
.footer_ul li a:focus {
  color: #ffc451;
  font-weight: normal;
}

.roleTooltip{position:absolute;left:0;top: -55px;opacity:0;visibility:hidden;transition:0.3s;display: inline-flex;border: 3px solid #fff;background-color:#FFC107;color: #333;font-family: 'GMARKETSANS';font-weight: bold;padding: 14px 10px 10px;white-space: nowrap;align-items: center;line-height: 1;border-radius: 3px;vertical-align: middle;}

.footer_ul li a:hover  + .roleTooltip,.footer_ul li a:focus  + .roleTooltip{opacity:1; visibility:visible}

