@charset "UTF-8";


.box{display:block;}
.dashboard{display:flex;flex-wrap:wrap;width: 100%;}
.box_inner{}
.box_inner.chart_box{height: 100%;display: inline-flex;width:100%;max-height: 470px;justify-content: space-between;}
.box_inner.chart_box > div {width:100%;max-width: calc(50% - 300px);height:100%;}
.box_inner.chart_box .left{}
.box_inner.chart_box .middle{}
.box_inner.chart_box .right{}
.box_inner.chart_box canvas{height:100% !important}

.box_inner.table_box{margin-top: 50px;}
.box_inner.line_chart{display:block;}